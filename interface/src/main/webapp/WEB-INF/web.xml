<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee"
  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
  version="2.5">

  <servlet>
    <servlet-name>one</servlet-name>
    <servlet-class>main.controllers.OneSimulationServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>one</servlet-name>
    <url-pattern>/one</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>extraction</servlet-name>
    <servlet-class>main.controllers.ExtractionServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>extraction</servlet-name>
    <url-pattern>/extraction</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>multi</servlet-name>
    <servlet-class>main.controllers.MultiSimulationServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>multi</servlet-name>
    <url-pattern>/multi</url-pattern>
  </servlet-mapping>


</web-app>
